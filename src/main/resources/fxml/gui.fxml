<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
            onKeyPressed="Space_Button_Pressed" prefHeight="600.0" prefWidth="600.0"
            xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="hu.unideb.inf.Controller.Controller">
    <children>
        <TabPane layoutY="20.0" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
                 prefHeight="500.0" prefWidth="230.0" tabClosingPolicy="UNAVAILABLE">
            <tabs>
                <Tab text="Year">
                    <content>
                        <AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
                                    minWidth="-Infinity" prefHeight="500.0" prefWidth="230.0">
                            <children>
                                <ListView fx:id="yearList" layoutY="30.0" maxHeight="-Infinity" maxWidth="-Infinity"
                                          minHeight="-Infinity" minWidth="-Infinity" prefHeight="460.0"
                                          prefWidth="220.0"/>
                                <Button fx:id="search_year_button" layoutX="165.0" layoutY="3.0" maxHeight="-Infinity"
                                        maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
                                        mnemonicParsing="false" text="Search"/>
                                <ChoiceBox layoutX="8.0" layoutY="2.0" maxHeight="-Infinity" maxWidth="-Infinity"
                                           minHeight="-Infinity" minWidth="-Infinity" prefWidth="150.0"/>
                            </children>
                        </AnchorPane>
                    </content>
                </Tab>
                <Tab text="Artist">
                    <content>
                        <AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
                                    minWidth="-Infinity" prefHeight="500.0" prefWidth="230.0">
                            <children>
                                <TextField layoutX="8.0" layoutY="2.0" maxHeight="-Infinity" maxWidth="-Infinity"
                                           minHeight="-Infinity" minWidth="-Infinity" prefWidth="150.0"/>
                                <ListView fx:id="artistList" layoutY="30.0" maxHeight="-Infinity" maxWidth="-Infinity"
                                          minHeight="-Infinity" minWidth="-Infinity" prefHeight="460.0"
                                          prefWidth="220.0"/>
                                <Button fx:id="search_artist_button" layoutX="165.0" layoutY="3.0" maxHeight="-Infinity"
                                        maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
                                        mnemonicParsing="false" text="Search"/>
                            </children>
                        </AnchorPane>
                    </content>
                </Tab>
                <Tab text="Album">
                    <content>
                        <AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
                                    minWidth="-Infinity" prefHeight="500.0" prefWidth="230.0">
                            <children>
                                <TextField layoutX="8.0" layoutY="2.0" maxHeight="-Infinity" maxWidth="-Infinity"
                                           minHeight="-Infinity" minWidth="-Infinity" prefWidth="150.0"/>
                                <ListView fx:id="albumList" layoutY="30.0" maxHeight="-Infinity" maxWidth="-Infinity"
                                          minHeight="-Infinity" minWidth="-Infinity" prefHeight="460.0"
                                          prefWidth="220.0"/>
                                <Button fx:id="search_album_button" layoutX="165.0" layoutY="3.0" maxHeight="-Infinity"
                                        maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
                                        mnemonicParsing="false" text="Search"/>
                            </children>
                        </AnchorPane>
                    </content>
                </Tab>
                <Tab text="Song">
                    <content>
                        <AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
                                    minWidth="-Infinity" prefHeight="500.0" prefWidth="230.0">
                            <children>
                                <ListView fx:id="songList" layoutY="30.0" maxHeight="-Infinity" maxWidth="-Infinity"
                                          minHeight="-Infinity" minWidth="-Infinity" prefHeight="460.0"
                                          prefWidth="220.0"/>
                                <Button fx:id="search_song_button" layoutX="165.0" layoutY="3.0" maxHeight="-Infinity"
                                        maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
                                        mnemonicParsing="false" text="Search"/>
                                <TextField layoutX="8.0" layoutY="2.0" maxHeight="-Infinity" maxWidth="-Infinity"
                                           minHeight="-Infinity" minWidth="-Infinity" prefWidth="150.0"/>
                            </children>
                        </AnchorPane>
                    </content>
                </Tab>
                <Tab text="Genre">
                    <content>
                        <AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
                                    minWidth="-Infinity" prefHeight="500.0" prefWidth="230.0">
                            <children>
                                <ChoiceBox layoutX="8.0" layoutY="2.0" maxHeight="-Infinity" maxWidth="-Infinity"
                                           minHeight="-Infinity" minWidth="-Infinity" prefWidth="150.0"/>
                                <ListView fx:id="genreList" layoutY="30.0" maxHeight="-Infinity" maxWidth="-Infinity"
                                          minHeight="-Infinity" minWidth="-Infinity" prefHeight="460.0"
                                          prefWidth="220.0"/>
                                <Button fx:id="search_genre_button" layoutX="165.0" layoutY="3.0" maxHeight="-Infinity"
                                        maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
                                        mnemonicParsing="false" text="Search"/>
                            </children>
                        </AnchorPane>
                    </content>
                </Tab>
            </tabs>
        </TabPane>
        <ListView fx:id="playlistList" layoutX="220.0" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
                  minWidth="-Infinity" nodeOrientation="LEFT_TO_RIGHT" orientation="HORIZONTAL" prefHeight="520.0"
                  prefWidth="380.0"/>
        <AnchorPane layoutY="520.0" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
                    minWidth="-Infinity" prefHeight="80.0" prefWidth="600.0">
            <children>
                <Button fx:id="play_button" defaultButton="true" layoutX="275.0" layoutY="25.0"
                        maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
                        mnemonicParsing="false" prefHeight="50.0" prefWidth="50.0" onMouseClicked="Play_Button_Pressed">
                    <graphic>
                        <ImageView fx:id="play_button_image" fitHeight="30.0" fitWidth="30.0" pickOnBounds="true"
                                   preserveRatio="true">
                            <image>
                                <Image fx:id="playImage" url="@../Images/playButt.png"/>
                            </image>
                        </ImageView>
                    </graphic>
                </Button>
                <Button fx:id="prev_button" layoutX="240.0" layoutY="35.0" maxHeight="-Infinity" maxWidth="-Infinity"
                        minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" prefHeight="30.0"
                        prefWidth="30.0">
                    <graphic>
                        <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                            <image>
                                <Image url="@../Images/prevButt.png"/>
                            </image>
                        </ImageView>
                    </graphic>
                </Button>
                <Button fx:id="next_button" layoutX="330.0" layoutY="35.0" maxHeight="-Infinity" maxWidth="-Infinity"
                        minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" prefHeight="30.0"
                        prefWidth="30.0">
                    <graphic>
                        <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                            <image>
                                <Image url="@../Images/nextButt.png"/>
                            </image>
                        </ImageView>
                    </graphic>
                </Button>
                <Button fx:id="shuffle_button" layoutX="210.0" layoutY="40.0" maxHeight="-Infinity" maxWidth="-Infinity"
                        minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" prefHeight="20.0"
                        prefWidth="20.0">
                    <graphic>
                        <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                            <image>
                                <Image fx:id="shuffleImage" url="@../Images/shuffle.png"/>
                            </image>
                        </ImageView>
                    </graphic>
                </Button>
                <Button fx:id="mute_button" alignment="BOTTOM_CENTER" contentDisplay="CENTER" layoutX="370.0"
                        layoutY="40.0" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
                        minWidth="-Infinity" mnemonicParsing="false" prefHeight="20.0" prefWidth="20.0">
                    <graphic>
                        <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                            <image>
                                <Image fx:id="muteImage" url="@../Images/muteButt.png"/>
                            </image>
                        </ImageView>
                    </graphic>
                </Button>
                <Button fx:id="loop_button" layoutX="180.0" layoutY="40.0" maxHeight="-Infinity" maxWidth="-Infinity"
                        minHeight="-Infinity"
                        minWidth="-Infinity" mnemonicParsing="false" prefHeight="20.0" prefWidth="20.0">
                    <graphic>
                        <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                            <image>
                                <Image fx:id="loopImage" url="@../Images/loopButt.png"/>
                            </image>
                        </ImageView>
                    </graphic>
                </Button>
                <Slider fx:id="play_slider" maxHeight="-Infinity" maxWidth="-Infinity"
                        minHeight="-Infinity" minWidth="-Infinity" prefHeight="20.0" prefWidth="600.0"/>
            </children>
        </AnchorPane>
        <MenuBar maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="20.0"
                 prefWidth="600.0">
            <menus>
                <Menu mnemonicParsing="false" text="File">
                    <items>
                        <MenuItem mnemonicParsing="false" text="Close"/>
                    </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Edit">
                    <items>
                        <MenuItem mnemonicParsing="false" text="Delete"/>
                    </items>
                </Menu>
            </menus>
        </MenuBar>
    </children>
</AnchorPane>
